{
  "name": "mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:prebuild": "expo prebuild --clean --platform android",
    "android:dev": "expo run:android --device",
    "apk": "cd android && ./gradlew assembleRelease",
